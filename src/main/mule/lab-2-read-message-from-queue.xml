	<anypoint-mq:config name="Anypoint_MQ_Config" doc:name="Anypoint MQ Config" doc:id="97f8c99c-e78c-406e-b4e2-61be22b735e5" >
		<anypoint-mq:connection url="${MQ.URL}" clientId="${MQ.clientID}" clientSecret="${MQ.clientSecret}"/>
